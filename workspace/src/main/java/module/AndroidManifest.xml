<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.yijian.workspace">


    <application
        android:name=".module.WorkSpaceApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher"
        android:supportsRtl="true"
        >
        <activity
            android:name=".static_assessment.StaticPhotoActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".static_assessment.StaticAssessmentActivity"
            android:launchMode="singleTask"
            android:screenOrientation="portrait" />
        <activity
            android:name=".static_assessment.StaticAssessmentActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".dynamic_assessment.DynamicAssessmentActivity"
            android:screenOrientation="portrait"
            android:launchMode="singleTask"/>
        <activity
            android:name=".dynamic_assessment.DynamicPhotoActivity"
            android:screenOrientation="portrait" />
        <activity android:name=".dynamic_assessment.DynamicDragPointActivity"
            android:screenOrientation="portrait"/>
    </application>



</manifest>
